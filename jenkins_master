pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger'
                script {
                    releasePackages = readJSON file: "${env.WORKSPACE}/data.json"
                    echo "$releasePackages.name"
                }
                
            }
        }
    }
}
